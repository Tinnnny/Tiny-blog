#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -x "$basedir/node" ]; then
  "$basedir/node"  "$basedir/../../../_hogan.js@3.0.2@hogan.js/bin/hulk" "$@"
  ret=$?
else 
  node  "$basedir/../../../_hogan.js@3.0.2@hogan.js/bin/hulk" "$@"
  ret=$?
fi
exit $ret
